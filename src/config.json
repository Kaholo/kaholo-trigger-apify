{
  "name": "Apify-Trigger",
  "type": "trigger",
  "execProgram": "node",
  "main": "app.js",
  "imgUrl": "logo.png",
  "version": "1.0.0",
  "description": "Handles Apify webhooks",
  "methods": [
    {
      "name": "runCreated",
      "route": "post /webhook/apify/process",
      "viewName": "Run Created",
      "params": []
    },
    {
      "name": "runSuccessed",
      "route": "post /webhook/apify/process",
      "viewName": "Run Succeeded",
      "params": []
    },
    {
      "name": "runFailed",
      "route": "post /webhook/apify/process",
      "viewName": "Run Failed",
      "params": []
    },
    {
      "name": "runAborted",
      "route": "post /webhook/apify/process",
      "viewName": "Run Timed Out",
      "params": []
    },
    {
      "name": "runTimedout",
      "route": "post /webhook/apify/process",
      "viewName": "Run Aborted",
      "params": []
    },
    {
      "name": "runResurrected",
      "route": "post /webhook/apify/process",
      "viewName": "Run Resurrected",
      "params": []
    }
  ]
}